@using Munchkin_Online.Models
@model HashSet<Munchkin_Online.Models.User>

@if (Model.Count != 0)
{
    foreach (User friend in Model)
    {
        <a @(friend.State == State.Offline ? "class=offline" : "") data-id="@friend.Id" href="/profile/@friend.Id.ToString("N")">@friend.Nickname</a>
    }
}
else
{
    <p>No friends</p>
    <p>Cry</p>
}